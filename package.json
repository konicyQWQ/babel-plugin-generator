{
  "name": "babel-plugin-generator-project",
  "version": "0.1.0",
  "description": "a babel plugin that can transform generator function to state machine, which is a ported version of typescript generator transform",
  "repository": {
    "url": "https://github.com/konicyQWQ/babel-plugin-generator"
  },
  "scripts": {
    "test": "ts-node ./test/regenerator/run.ts && jest",
    "core:build": "cd packages/core && npm run build",
    "runtime:build": "cd packages/runtime && npm run build",
    "docs:build": "cd packages/playground && npm run build",
    "build": "npm run core:build && npm run runtime:build && npm run docs:build",
    "dev": "cd packages/playground && npm run start",
    "ci": "rm -rf **/node_modules && pnpm install"
  },
  "keywords": [
    "babel-plugin",
    "generator function"
  ],
  "author": "konicyqwq",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@types/node": "^16.11.43",
    "babel-plugin-generator": "workspace:^0.1.8",
    "babel-plugin-generator-runtime": "workspace:^0.1.1",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "@babel/plugin-transform-classes": "^7.18.6",
    "@babel/plugin-transform-destructuring": "^7.18.6",
    "@babel/plugin-transform-for-of": "^7.18.6",
    "@babel/plugin-transform-parameters": "^7.18.6",
    "@babel/plugin-transform-spread": "^7.18.6",
    "@babel/plugin-transform-block-scoping": "^7.18.6",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/babel__template": "^7.4.1",
    "@types/jest": "^28.1.3",
    "babel-plugin-tester": "^10.1.0"
  }
}
